{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "csg",
      "collectionName": "Coasters",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionString"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "csg",
      "collectionName": "Coasters",
      "connectionStringSetting": "CosmosDbConnectionString",
      "id": "{Query.id}",
      "partitionKey": "{Query.id}"
    }
  ],
  "scriptFile": "../dist/createCoaster/index.js"
}
